import java.awt.Container;
import java.awt.GridBagLayout;

import javax.swing.*;

public class GUI extends JFrame 
{

	public static void main(String[] args) 
	{
		new GUI();
	}
	
	public GUI()
	{		
		setSize(300, 300);
			setVisible(true);
				Container container= getContentPane();
				container.setLayout(new GridBagLayout());;
				
				JLabel jlbGUI = new JLabel("My name is: ");
				add(jlbGUI);
				setVisible(true);
				
				JTextField jtfGUI = new JTextField(10);
				jtfGUI.getText();
				container.add(jtfGUI);
				setVisible(true);
				jtfGUI.setEditable(true);
				
				JLabel jlbGUI1 = new JLabel("I am: ");
				add(jlbGUI1);
				setVisible(true);
	}
}
