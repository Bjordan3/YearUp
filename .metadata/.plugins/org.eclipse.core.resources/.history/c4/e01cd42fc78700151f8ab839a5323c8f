
public class ExceptionExample {

	public static void main(String[] args) {
		int[] numbers =  {1,3,5,7};
		
		AddArray(numbers);
	}

	private static void AddArray(int[] numbers)
	{
		int sum = 0;
		
		for (int i = 0; i < numbers.length; i++)
		{
			sum = sum + numbers[i];
		}
		System.out.println("Sum is: " + sum);
	}
}
