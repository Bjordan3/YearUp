import java.awt.Container;
import java.awt.FlowLayout;

import javax.swing.*;

public class GUI extends JFrame 
{

	public static void main(String[] args) 
	{
		new GUI();
	}
	
	public GUI()
	{		
		setSize(300, 300); 
				Container container= getContentPane();
				container.setLayout(new FlowLayout());;
				
				Container container2 = getContentPane();
				container2.setLayout(new FlowLayout());
				
				Container container3 = getContentPane();
				container3.setLayout(new FlowLayout());
				
				JLabel jlbGUI = new JLabel("My name is: ");
				container.add(jlbGUI);

				JTextField jtfGUI = new JTextField(10);
				jtfGUI.getText();
				container.add(jtfGUI);
				jtfGUI.setEditable(true);
				
				JLabel jlbGUI1 = new JLabel("I am: ");
				add(jlbGUI1);
				container2.add(jlbGUI);
				
				setVisible(true);
	}
}
