import java.awt.Container;
import java.awt.FlowLayout;
import java.awt.GridLayout;
import java.awt.Panel;

import javax.swing.*;

public class GUI extends JFrame 
{

	public static void main(String[] args) 
	{
		new GUI();
	}
	
	public GUI()
	{		
		super("Hello");
		
		setSize(250, 250); 
				
		setLayout(new FlowLayout());
		
		Panel panel = new Panel();
		setLayout(new FlowLayout());
		
				JLabel jlbGUI = new JLabel("My name is: ");
				panel.add(jlbGUI);

				JTextField jtfGUI = new JTextField(10);
				jtfGUI.getText();
				panel.add(jtfGUI);
				jtfGUI.setEditable(true);
				
				JLabel jlbGUI1 = new JLabel("I am: ");
				panel.add(jlbGUI1);
				
				setVisible(true);
	}
}
