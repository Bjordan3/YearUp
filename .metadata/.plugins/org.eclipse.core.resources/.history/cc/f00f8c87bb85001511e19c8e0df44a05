public class Product{
public String Name;
public double Price;
public double WithTax;

public Product(String name, double price, double withTax)
{
	Name = name;
	Price = price;
	double tax = .075;
	withTax = price * tax + price;
}
public Product(String name, double price)
{
	Name = name;
	Price = price;
}

public static void add(String name, double price)
{
 
}


public String getName()
{
	return Name;
}

public double getPrice()
{
	return Price;
}

public void setWithTax(double withTax) 
{
	WithTax = withTax;
}

public double getWithTax() 
{
	return WithTax;
}

}
